@{
    ViewData["Title"] = "Index";
}

<h2>DOTNETCORE for OPENSHIFT</h2>
<h3>OPENSHIFT with us</h3>
<p>Docker with us</p>

<h1> Role </h1>
<table>
    <tr>
        <th> Role </th>
        <th style="padding-left:100px;"> Name </th>
    </tr>
    @foreach (var student in Model)
    {
        <tr>
            <td>@student.Role</td> 
            <td style="padding-left:100px;">@student.Name</td>
        </tr>
    }
</table>